{
  "version": 2,
  "name": "ymkz.now.sh",
  "alias": "ymkz.now.sh",
  "regions": ["hnd1"],
  "github": {
    "silent": true
  },
  "builds": [
    { "src": "next.config.js", "use": "@now/next" },
    { "src": "tests/index.html", "use": "@now/static" },
    { "src": "tests/integration.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/tests", "dest": "/tests/index.html" },
    { "src": "/tests/(.+)", "dest": "/tests/integration.js" }
  ]
}
