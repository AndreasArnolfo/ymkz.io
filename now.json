{
  "version": 2,
  "name": "ymkz.io",
  "alias": "ymkz.io",
  "regions": ["hnd1"],
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/images|pages|chunks|runtime)/.+",
      "headers": { "cache-control": "immutable, max-age=31536000" }
    },
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "immutable, max-age=31536000" }
    }
  ]
}
